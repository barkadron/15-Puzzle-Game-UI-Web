{
    "compilerOptions": {
        "strict": true,
        "target": "es5",
        "module": "CommonJS",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "importHelpers": true,
        "removeComments": true,
        "sourceMap": true,
        "allowJs": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
    },
    "include": ["src"]
}
